            },
            {
              "text": "Contact",
              "icon": {
                "url": "/assets/support.png",
                "tint": false
              },
              "action": "navpage://Contact",
              "backgroundcolor": "#1A00C2FF"
            }
          ]
        }
      ]
    }
  },
  "triggers": [
    {
      "type": "Reminder",
      "content": {
        "path": "https://hdsp.co/share/681b905c676f43eaa24fb490",
        "prompt": "We noticed you haven’t completed Meditation Basics in the past couple of days. Please try to fit one in today—just click the mindfulness tile in the app. If you’re having trouble accessing it, let us know so we can help.",
        "threshold": "2.00:00:00",
        "type": "click"
      }
    },
    {
      "type": "Reminder",
      "content": {
        "path": "https://hdsp.co/share/681baa3bb7ef4f17b820ccf6",
        "prompt": "We noticed you haven’t completed Sports|Recovery Course in the past couple of days. Please try to fit one in today—just click the mindfulness tile in the app. If you’re having trouble accessing it, let us know so we can help.",
        "threshold": "2.00:00:00",
        "type": "click"
      }
    },
    {
      "type": "Reminder",
      "content": {
        "path": "https://hdsp.co/share/6839e11d7419d4b1882761d7",
        "prompt": "We noticed you haven’t completed Sports|Rehab Course in the past couple of days. Please try to fit one in today—just click the mindfulness tile in the app. If you’re having trouble accessing it, let us know so we can help.",
        "threshold": "00:05:00",
        "type": "click"
      }
    },
    {
      "type": "Service",
      "frequency": "1",
      "time": "08:00",
      "content": {
        "path": "https://api.ouraring.com/v2/usercollection",
        "service": "Oura",
        "endpoints": [
          {
            "endpoint": "daily_activity",
            "prompt": "Hi! We’ve noticed your tracker hasn’t recorded much data over the past two days. Please try to wear it as much as possible—ideally all day. If it’s not working or you’re having trouble, let us know so we can help.",
            "pass_through_only": false,
            "time_window_days": 1,
            "aggregation": "sum",
            "condition": [
              "non_wear_time",
              ">",
              "36000",
              "steps",
              "<",
              "2000"
            ]
          },
          {
            "endpoint": "sleep",
            "pass_through_only": true
          },
          {
            "endpoint": "workout",
            "pass_through_only": true
          }
        ]
      }
    },
    {
      "type": "random",
      "days": 7,
      "action": "notification",
      "actions": [
        {
          "time": "10:00",
          "path": "flow: //flows/survey1",
          "expiry": "11:00"
        },
        {
          "time": "04:00",
          "open": "03:00",
          "close": "06:00",
          "path": "flow://flows/survey2",
          "expiry": "11:00"
        },
        {
          "time": "11:00",
          "path": "flow://flows/survey3"
        }
      ]
    }
  ],
  
}
