        {
          "type": "goals",
          "goals": [
            {
              "service": "Oura",
              "requires-auth": true,
              "endpoint": "https://api.ouraring.com/v2/usercollection/daily_activity",
              "android-sync": "deeplink://com.ouraring.oura",
              "ios-sync": "deeplink://Oura://",
              "type": "percentile",
              "show-loading": true,
              "field": "active",
              "days": 10,
              "percentile": .6
            }
          ]
        },
        {
          "type": "carousel",
          "actions": [
            {
              "text": "Meditation Basics",
              "icon": {
